<Window x:Class="Bank_lukasz_niescierewski.AccountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NoweKonto" Height="150" Width="392">
    <Grid>
        <Label Content="Wygenerowany numer:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="136"/>
        <Label Content="Rodzaj konta:" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Width="136"/>
        <TextBox x:Name="TB_nraccount_generate" HorizontalAlignment="Left" Height="23" Margin="151,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="226" IsEnabled="False"/>
        <ComboBox x:Name="CB_type_account" HorizontalAlignment="Left" Margin="151,54,0,0" VerticalAlignment="Top" Width="226">
            <ComboBoxItem Content="ROR"/>
            <ComboBoxItem Content="Lokata"/>
            <ComboBoxItem Content="Karta kredytowa"/>
        </ComboBox>
        <Button x:Name="B_cancel_account" Content="Anuluj" HorizontalAlignment="Left" Margin="326,88,0,0" VerticalAlignment="Top" Width="50" Click="B_cancel_account_Click"/>
        <Button x:Name="B_ok_account" Content="OK" HorizontalAlignment="Left" Margin="291,88,0,0" VerticalAlignment="Top" Width="30" Click="B_ok_account_Click"/>

    </Grid>
</Window>
